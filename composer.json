{
    "name": "benlumia007/inheritance",
    "description": "Inheritance is the next level theme to support ClassicPress.",
    "keywords": [
        "classicpress",
        "classicpress-theme"
    ],
    "homepage": "https://github.com/benlumia007/inheritance",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Benjamin Lu",
            "email": "benlumia007@gmail.com",
            "homepage": "https://benjlu.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Inheritance\\": "app/"
		},
		"files":[
			"app/Theme/Entry/functions-entry.php",
			"app/framework.php",
			"app/functions-filters.php",
			"app/functions-scripts.php",
			"app/functions-setup.php"
		]
    },
    "require": {
        "php": ">=7.4.29",
        "benlumia007/backdrop": "^2.0.0",
		"benlumia007/backdrop-fontawesome": "^1.0.0",
		"benlumia007/backdrop-fonts": "^1.0.0",
		"benlumia007/backdrop-mix-manifest": "^1.0.0",
		"benlumia007/backdrop-template-hierarchy": "^1.0.0",
		"benlumia007/backdrop-template-manager": "^1.0.0",
		"benlumia007/backdrop-template-view": "^1.0.0",
		"benlumia007/backdrop-theme": "^1.0.0"
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}